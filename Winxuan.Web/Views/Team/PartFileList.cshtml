<table class="table table-striped">
    <caption>@ViewBag.TeamName</caption>

    <thead>
        <tr>
            <th>文件名称</th>
            <th>操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in ViewBag.FileList)
        {
            <tr>
                <td><div class="hidMsgInfo" title="@data.TeamName">@data.TeamName</div></td>
                <td><button class="btn btn-info">下载/button></td>
            </tr>
        }
    </tbody>
</table>